{
    "name": "retentio/boomstone",
    "type": "library",
    "description": "Boomstone, a spark for PHP project",
    "keywords": ["silex", "boomgo", "bootstrap", "mongodb"],
    "homepage": "https://github.com/Retentio/Boomstone",
    "license": "MIT",
    "authors": [
        {
            "name": "Ludovic Fleury",
            "email": "ludo.fleury@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
                "version": "2.0.1",
                "source": {
                    "url": "git://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "v2.0.1"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "retentio/boomgo": "dev-repository",
        "silex/silex": "1.0.*",
        "symfony/finder": "2.1.*",
        "symfony/translation": "2.1.*",
        "symfony/twig-bridge": "2.1.*",
        "symfony/form": "2.1.*",
        "symfony/validator": "2.1.*",
        "symfony/config": "2.1.*",
        "symfony/yaml": "2.1.*",
        "swiftmailer/swiftmailer": "4.1.5",
        "twitter/bootstrap": "2.0.1"
    },
    "autoload": {
        "psr-0": {
            "Boomstone": "src/"
        }
    }
}